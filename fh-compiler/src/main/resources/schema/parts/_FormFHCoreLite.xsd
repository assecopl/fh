<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <xs:annotation>
        <xs:documentation>
            <![CDATA[
              <p>The XML Schema for the FH Form configuration file</p>

              <p>All FH Forms configuration files must indicate
              the FH Form schema by indicating the FH namespace:</p>

              <p>http://fh.asseco.com/form/1.0</p>
        ]]>
        </xs:documentation>
    </xs:annotation>

    <!-- =====================================================================================================
    Form configuration
    ===================================================================================================== -->
    <xs:element name="Form" type="FormType"/>
    <xs:complexType name="FormType">
        <xs:choice>
            <xs:element type="AvailabilityConfigurationType" name="AvailabilityConfiguration">
                <xs:unique name="uniqueVariantID">
                    <xs:selector xpath=".//*"/>
                    <xs:field xpath="@id"/>
                </xs:unique>
            </xs:element>
            <xs:element type="ModelType" name="Model" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="AccordionType" name="Accordion" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="ButtonType" name="Button" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="ButtonGroupType" name="ButtonGroup" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="CalendarType" name="Calendar" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="CheckBoxType" name="CheckBox" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="ComboType" name="Combo" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="DictionaryComboType" name="DictionaryCombo" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="PanelGroupType" name="PanelGroup" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="FloatingGroupType" name="FloatingGroup" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="ImageType" name="Image" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="MdFileViewerType" name="MdFileViewer" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="IncludeType" name="Include" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="InputDateType" name="InputDate" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="InputDateOptimizedType" name="InputDateOptimized" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="InputNumberType" name="InputNumber" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="InputTextType" name="InputText" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="InputTimestampType" name="InputTimestamp" v/>
            <xs:element type="LinkType" name="Link" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="LocaleBundleType" name="LocaleBundle" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="OutputLabelType" name="OutputLabel" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="OptionsListType" name="OptionsList" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="RadioOptionType" name="RadioOption" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="RadioOptionsGroupType" name="RadioOptionsGroup" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="RepeaterType" name="Repeater" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TimerType" name="Timer" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="KeyboardEventType" name="KeyboardEvent" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="GroupType" name="Group" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="HtmlEditorType" name="HtmlEditor" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="HtmlViewType" name="HtmlView" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="EmbedPageType" name="EmbedPage" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="SelectOneMenuType" name="SelectOneMenu" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenu" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenuOptimized" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="SpacerType" name="Spacer" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="SplitContainerType" name="SplitContainer" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TabContainerType" name="TabContainer" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TablePagedType" name="TablePaged" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TableLazyType" name="TableLazy" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TableOptimizedType" name="TableOptimized" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TableType" name="Table" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="TreeType" name="Tree" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="ImageGridType" name="ImageGrid" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="MarkdownGridType" name="MarkdownGrid" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="ValidateMessagesType" name="ValidateMessages" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="FileUploadType" name="FileUpload" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="WidgetType" name="Widget" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="WizardType" name="Wizard" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="DashboardType" name="Dashboard" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element name="Comment" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="IMapLiteType" name="IMapLite" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="OpenLayersType" name="OpenLayers" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="DropdownType" name="Dropdown" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="RowType" name="Row" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="AnchorType" name="Anchor" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element type="UmlDiagramType" name="UmlDiagram" maxOccurs="unbounded" minOccurs="0"/>
        </xs:choice>
        <xs:attribute type="xs:string" name="container"/>
        <xs:attribute type="xs:string" name="id"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="FormPossibleTypes" name="formType" default="STANDARD"/>
        <xs:attribute type="FormModalPossibleSizes" name="modalSize" default="REGULAR"/>
        <xs:attribute type="xs:boolean" name="hideHeader"/>
        <xs:attribute type="HeadingMode" name="headingType"/>
        <xs:attribute type="xs:integer" name="modalMaxHeight"/>
        <xs:attribute type="xs:boolean" name="focusFirstElement" default="true"/>
        <xs:attribute type="xs:string" name="onManualModalClose"/>
    </xs:complexType>

    <xs:simpleType name="AvailabilityConfigurationElementType">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="AvailabilityConfigurationElementTagType">
        <xs:simpleContent>
            <xs:extension base="AvailabilityConfigurationElementType">
                <xs:attribute name="when" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="VariantType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="AvailabilityValueType" name="AvailabilityValue"/>
            <xs:element type="AvailabilityType" name="Availability"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="Invisible"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="Preview"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="Edit"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="ReadOnly"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="SetByProgrammer"/>
        </xs:choice>
        <xs:attribute name="id" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:minLength value="1"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute type="DefaultAvailabilityType" name="defaultAvailability" default="EDIT"/>
    </xs:complexType>

    <xs:complexType name="AvailabilityValueType">
        <xs:simpleContent>
            <xs:extension base="AvailabilityConfigurationElementType">
                <xs:attribute name="value" type="xs:string" use="required"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="AvailabilityType">
        <xs:simpleContent>
            <xs:extension base="AvailabilityConfigurationElementType">
                <xs:attribute name="editPermissions" type="xs:string"/>
                <xs:attribute name="previewPermissions" type="xs:string"/>
                <xs:attribute name="invisiblePermissions" type="xs:string"/>
                <xs:attribute name="editRoles" type="xs:string"/>
                <xs:attribute name="previewRoles" type="xs:string"/>
                <xs:attribute name="invisibleRoles" type="xs:string"/>
                <xs:attribute name="default" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="AvailabilityConfigurationType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="VariantType" name="Variant"/>
            <xs:element type="AvailabilityValueType" name="AvailabilityValue"/>
            <xs:element type="AvailabilityType" name="Availability"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="Invisible"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="Preview"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="Edit"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="ReadOnly"/>
            <xs:element type="AvailabilityConfigurationElementTagType" name="SetByProgrammer"/>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="ModelType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="ModelPropertyType" name="Property"/>
        </xs:choice>
        <xs:attribute type="xs:string" name="externalClass"/>
    </xs:complexType>

    <xs:complexType name="DictionaryComboParameterType">
        <xs:attribute type="xs:string" name="name"/>
        <xs:attribute type="xs:string" name="value"/>
    </xs:complexType>

    <xs:complexType name="ParametersType">
        <xs:attribute type="xs:string" name="name"/>
        <xs:attribute type="xs:string" name="value"/>
    </xs:complexType>

    <xs:complexType name="ModelPropertyType">
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="type" type="xs:string" use="required"/>
        <xs:attribute name="multiplicity" type="PropertyMultiplicityType" default="single"/>
        <xs:attribute name="private" type="xs:boolean" default="false"/>
    </xs:complexType>

    <!-- =====================================================================================================
    Form components - reusable types
    ===================================================================================================== -->
    <xs:attributeGroup name="FormElementTypeGroup">
        <xs:attribute type="pixelOrPercentageType" name="height"/>
        <xs:attribute type="pixelType" name="marginLeft"/>
        <xs:attribute type="pixelType" name="marginRight"/>
        <xs:attribute type="pixelType" name="marginTop"/>
        <xs:attribute type="pixelType" name="marginBottom"/>
        <xs:attribute type="pixelType" name="paddingLeft"/>
        <xs:attribute type="pixelType" name="paddingRight"/>
        <xs:attribute type="pixelType" name="paddingTop"/>
        <xs:attribute type="pixelType" name="paddingBottom"/>
        <xs:attribute type="xs:ID" name="id"/>
        <xs:attribute type="xs:string" name="width" default="md-12"/>
        <xs:attribute type="xs:string" name="styleClasses" default=""/>
        <xs:attribute type="verticalAlignType" name="verticalAlign" default="bottom"/>
        <xs:attribute type="horizontalAlignType" name="horizontalAlign" default="left"/>
        <xs:attribute type="xs:string" name="hint"/>
        <xs:attribute type="xs:string" name="hintTitle"/>
        <xs:attribute type="xs:string" name="hintAriaLabel"/>
        <xs:attribute type="xs:string" name="hintIcon"/>
        <xs:attribute type="hintPlacementType" name="hintPlacement"/>
        <xs:attribute type="hintTriggerType" name="hintTrigger"/>
        <xs:attribute type="hintTypeType" name="hintType"/>
        <xs:attribute type="xs:string" name="availability"/>
        <xs:attribute type="xs:string" name="htmlAccessibilityRole"/>
    </xs:attributeGroup>

    <xs:attributeGroup name="RepeatableTypeGroup">
        <xs:attribute type="xs:string" name="collection" use="required"/>
        <xs:attribute type="xs:string" name="iterator" use="required"/>
    </xs:attributeGroup>

    <xs:attributeGroup name="PanelGroupTypeGroup">
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute type="xs:string" name="collapsible" default="false"/>
        <xs:attribute type="xs:string" name="borderVisible" default="false"/>
        <xs:attribute type="xs:string" name="collapsed" default="false"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="HeadingMode" name="headingType"/>
        <xs:attribute type="xs:string" name="onToggle"/>
        <xs:attribute type="xs:string" name="iconOpened" default="fa-arrow-down"/>
        <xs:attribute type="xs:string" name="iconClosed" default="fa-arrow-up"/>
    </xs:attributeGroup>

    <xs:attributeGroup name="FloatingGroupTypeGroup">
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute type="xs:string" name="collapsible" default="false"/>
        <xs:attribute type="xs:string" name="collapsed" default="false"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="xs:string" name="onToggle"/>
        <xs:attribute type="xs:string" name="onTogglePin"/>
        <xs:attribute type="xs:string" name="left"/>
        <xs:attribute type="xs:string" name="top"/>
        <xs:attribute type="xs:string" name="onToggleFullScreen"/>
        <xs:attribute type="PinningMode" name="pinningMode" default="normal"/>
        <xs:attribute type="xs:string" name="floatingState"/>
        <xs:attribute type="xs:string" name="floatingOnly"/>
        <xs:attribute type="xs:int" name="floatingHeight"/>
        <xs:attribute type="xs:int" name="floatingWidth"/>
        <xs:attribute type="xs:boolean" name="hideHeader" default="false"/>
        <xs:attribute type="xs:boolean" name="hideButtons" default="false"/>
        <xs:attribute type="xs:boolean" name="isDraggable" default="true"/>

    </xs:attributeGroup>

    <!-- =====================================================================================================
    Form components
    ===================================================================================================== -->

    <xs:complexType name="AccordionType" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="PanelGroupType" name="PanelGroup"/>
            <xs:element type="WidgetType" name="Widget"/>
            <xs:element type="FloatingGroupType" name="FloatingGroup"/>
            <xs:element type="GroupType" name="Group"/>
        </xs:choice>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute type="xs:string" name="activeGroup" default="0"/>
        <xs:attribute type="xs:string" name="iconOpened" default="fa-chevron-down"/>
        <xs:attribute type="xs:string" name="iconClosed" default="fa-chevron-up"/>
        <xs:attribute type="xs:string" name="onGroupChange"/>
    </xs:complexType>

    <xs:complexType name="BarChartType">
        <xs:attribute type="xs:string" name="values" use="required"/>
        <xs:attribute type="xs:string" name="id"/>
        <xs:attribute type="xs:string" name="width"/>
        <xs:attribute type="xs:string" name="title"/>
        <xs:attribute type="xs:string" name="xAxisLabel"/>
        <xs:attribute type="xs:anyURI" name="yAxisLabel"/>
        <xs:attribute type="xs:anyURI" name="yAxisMin"/>
        <xs:attribute type="xs:anyURI" name="yAxisMax"/>
        <xs:attribute type="xs:string" name="stacked"/>
        <xs:attribute type="xs:string" name="colors"/>
        <xs:attribute type="pixelOrPercentageType" name="height"/>
    </xs:complexType>

    <xs:complexType name="ButtonType" mixed="true">
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attributeGroup ref="ButtonTypeAttributeGroup"/>
        <xs:attributeGroup ref="ConfirmationDialogSupport"/>
        <xs:attribute type="xs:string" name="ariaLabel"/>
    </xs:complexType>

    <xs:attributeGroup name="ButtonTypeAttributeGroup">
        <xs:attribute type="xs:string" name="label" use="required"/>
        <xs:attribute type="xs:string" name="leftPadding"/>
        <xs:attribute type="xs:string" name="onClick"/>
        <xs:attribute type="xs:string" name="style"/>
    </xs:attributeGroup>

    <xs:attributeGroup name="ConfirmationDialogSupport">
        <xs:attribute type="xs:string" name="confirmationMsg"/>
        <xs:attribute type="xs:string" name="confirmOnEvent"/>
    </xs:attributeGroup>

    <xs:complexType name="ButtonGroupType" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="ButtonType" name="Button"/>
            <xs:element type="DropdownType" name="Dropdown"/>
        </xs:choice>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute type="xs:string" name="activeButton"/>
        <xs:attribute type="xs:boolean" name="margin" default="false"/>
        <xs:attribute type="xs:string" name="onButtonChange"/>
    </xs:complexType>

    <xs:complexType name="CalendarType">
        <xs:attributeGroup ref="ConfirmationDialogSupport"/>
        <xs:attribute type="xs:string" name="ariaLabel"/>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute type="xs:string" name="blockedDates"/>
        <xs:attribute type="xs:string" name="changeMonth" default="false"/>
        <xs:attribute type="xs:string" name="changeYear" default="false"/>
        <xs:attribute type="xs:string" name="icon"/>
        <xs:attribute type="xs:string" name="iconAlignment"/>
        <xs:attribute type="InputSizeType" name="inputSize"/>
        <xs:attribute type="LabelSizeType" name="labelSize"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="xs:string" name="validationLabel"/>
        <xs:attribute type="xs:boolean" name="hintInputGroup"/>
        <xs:attribute type="LabelPositionType" name="labelPosition"/>
        <xs:attribute type="xs:string" name="maxDate"/>
        <xs:attribute type="xs:string" name="minDate"/>
        <xs:attribute type="xs:string" name="onChange"/>
        <xs:attribute type="xs:string" name="required" default="false"/>
        <xs:attribute type="xs:string" name="value"/>
        <xs:attribute type="xs:string" name="values"/>
    </xs:complexType>

    <xs:complexType name="CheckBoxType">
        <xs:attributeGroup ref="ConfirmationDialogSupport"/>
        <xs:attribute type="xs:string" name="ariaLabel"/>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute type="xs:string" name="icon"/>
        <xs:attribute type="InputSizeType" name="inputSize"/>
        <xs:attribute type="LabelSizeType" name="labelSize"/>
        <xs:attribute type="xs:string" name="iconAlignment"/>
        <xs:attribute type="xs:string" name="label" use="required"/>
        <xs:attribute type="xs:string" name="autocomplete"/>
        <xs:attribute type="xs:string" name="validationLabel"/>
        <xs:attribute type="xs:boolean" name="hintInputGroup"/>
        <xs:attribute type="LabelPositionType" name="labelPosition"/>
        <xs:attribute type="xs:string" name="onChange"/>
        <xs:attribute type="xs:string" name="required" default="false"/>
        <xs:attribute type="xs:string" name="value"/>
        <xs:attribute type="xs:string" name="width" default="md-2"/>
        <xs:attribute type="xs:string" name="labelId"/>
    </xs:complexType>

    <xs:complexType name="ColumnType" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="ColumnType" name="Column"/>
            <xs:element type="ButtonType" name="Button"/>
            <xs:element type="PanelGroupType" name="PanelGroup"/>
            <xs:element type="CalendarType" name="Calendar"/>
            <xs:element type="CheckBoxType" name="CheckBox"/>
            <xs:element type="ComboType" name="Combo"/>
            <xs:element type="DictionaryComboType" name="DictionaryCombo"/>
            <xs:element type="GroupType" name="Group"/>
            <xs:element type="ImageType" name="Image"/>
            <xs:element type="InputDateType" name="InputDate"/>
            <xs:element type="InputDateOptimizedType" name="InputDateOptimized"/>
            <xs:element type="InputNumberType" name="InputNumber"/>
            <xs:element type="InputTextType" name="InputText"/>
            <xs:element type="InputTimestampType" name="InputTimestamp"/>
            <xs:element type="LinkType" name="Link"/>
            <xs:element type="MeterGaugeChartType" name="MeterGaugeChart"/>
            <xs:element type="OutputLabelType" name="OutputLabel"/>
            <xs:element type="RadioOptionType" name="RadioOption"/>
            <xs:element type="RadioOptionsGroupType" name="RadioOptionsGroup"/>
            <xs:element type="SelectOneMenuType" name="SelectOneMenu"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenu"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenuOptimized"/>
            <xs:element type="ThreeDotsMenuType" name="ThreeDotsMenu"/>
        </xs:choice>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute name="height" use="prohibited"/>
        <xs:attribute type="xs:string" name="iterationRef"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="xs:string" name="value"/>
    </xs:complexType>

    <xs:complexType name="ColumnPagedType" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="ColumnPagedType" name="ColumnPaged"/>
            <xs:element type="ColumnType" name="Column"/>
            <xs:element type="ButtonType" name="Button"/>
            <xs:element type="CalendarType" name="Calendar"/>
            <xs:element type="CheckBoxType" name="CheckBox"/>
            <xs:element type="ComboType" name="Combo"/>
            <xs:element type="DictionaryComboType" name="DictionaryCombo"/>
            <xs:element type="MdFileViewerType" name="MdFileViewer"/>
            <xs:element type="ImageType" name="Image"/>
            <xs:element type="InputDateType" name="InputDate"/>
            <xs:element type="InputDateOptimizedType" name="InputDateOptimized"/>
            <xs:element type="InputNumberType" name="InputNumber"/>
            <xs:element type="InputTextType" name="InputText"/>
            <xs:element type="InputTimestampType" name="InputTimestamp"/>
            <xs:element type="LinkType" name="Link"/>
            <xs:element type="MeterGaugeChartType" name="MeterGaugeChart"/>
            <xs:element type="OutputLabelType" name="OutputLabel"/>
            <xs:element type="RadioOptionType" name="RadioOption"/>
            <xs:element type="RadioOptionsGroupType" name="RadioOptionsGroup"/>
            <xs:element type="SelectOneMenuType" name="SelectOneMenu"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenu"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenuOptimized"/>
            <xs:element type="ThreeDotsMenuType" name="ThreeDotsMenu"/>
        </xs:choice>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute name="height" use="prohibited"/>
        <xs:attribute type="xs:string" name="sortBy"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="xs:string" name="value"/>
    </xs:complexType>

    <xs:complexType name="ColumnLazyType" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="ColumnPagedType" name="ColumnPaged"/>
            <xs:element type="ColumnType" name="Column"/>
            <xs:element type="ButtonType" name="Button"/>
            <xs:element type="CalendarType" name="Calendar"/>
            <xs:element type="CheckBoxType" name="CheckBox"/>
            <xs:element type="ComboType" name="Combo"/>
            <xs:element type="DictionaryComboType" name="DictionaryCombo"/>
            <xs:element type="MdFileViewerType" name="MdFileViewer"/>
            <xs:element type="ImageType" name="Image"/>
            <xs:element type="InputDateType" name="InputDate"/>
            <xs:element type="InputDateOptimizedType" name="InputDateOptimized"/>
            <xs:element type="InputNumberType" name="InputNumber"/>
            <xs:element type="InputTextType" name="InputText"/>
            <xs:element type="InputTimestampType" name="InputTimestamp"/>
            <xs:element type="LinkType" name="Link"/>
            <xs:element type="MeterGaugeChartType" name="MeterGaugeChart"/>
            <xs:element type="OutputLabelType" name="OutputLabel"/>
            <xs:element type="RadioOptionType" name="RadioOption"/>
            <xs:element type="RadioOptionsGroupType" name="RadioOptionsGroup"/>
            <xs:element type="SelectOneMenuType" name="SelectOneMenu"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenu"/>
            <xs:element type="SelectComboMenuType" name="SelectComboMenuOptimized"/>
        </xs:choice>
        <xs:attributeGroup ref="FormElementTypeGroup"/>
        <xs:attribute name="height" use="prohibited"/>
        <xs:attribute type="xs:string" name="sortBy"/>
        <xs:attribute type="xs:string" name="label"/>
        <xs:attribute type="xs:string" name="value"/>
    </xs:complexType>

    <xs:complexType name="ColumnOptimizedType" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element type="ColumnPagedType" name="ColumnPaged"/>
            <xs:element type="ColumnType" name="Column"/>
            <xs:element type="ButtonType" name="Button"/>
            <xs:element type="CalendarType" name="Calendar"/>
            <xs:element type="CheckBoxType" name="CheckBox"/>
            <xs:element type="ComboType" name="Combo"/>
            <xs:element type="DictionaryComboType" name="DictionaryCombo"/>
            <xs:element type="MdFileViewerType" name="MdFileViewer"/>
            <xs:element type="ImageType" name="Image"/>
            <xs:element type="InputDateType" name="InputDate"/>
