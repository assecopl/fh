{
  "name": "fh-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/fh-angular/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/fh-angular",
        "index": "apps/fh-angular/src/index.html",
        "main": "apps/fh-angular/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/fh-angular/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/fh-angular/src/favicon.ico",
          "apps/fh-angular/src/assets"
        ],
        "styles": [
          "apps/fh-angular/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "libs/fh-ng-core-lite/src/styles"
          ]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "buildExample": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "../../fhdp/fhdp-example/target/classes/static/angular",
        "index": "apps/fh-angular/src/index.html",
        "main": "apps/fh-angular/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/fh-angular/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/fh-angular/src/favicon.ico",
          "apps/fh-angular/src/assets"
        ],
        "styles": [
          "apps/fh-angular/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": [
            "libs/fh-ng-core-lite/src/styles"
          ]
        },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },

    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "fh-angular:build:production"
        },
        "development": {
          "browserTarget": "fh-angular:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "fh-angular:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ],
        "tsConfig": "apps/fh-angular/tsconfig.spec.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/fh-angular/src/favicon.ico",
          "apps/fh-angular/src/assets"
        ],
        "styles": [
          "apps/fh-angular/src/styles.scss"
        ],
        "scripts": []
      }
    }
  }
}
