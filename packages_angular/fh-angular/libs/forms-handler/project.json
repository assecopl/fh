{
  "name": "forms-handler",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/forms-handler/src",
  "prefix": "lib",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "tags": [
    "fh-ng"
  ],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/forms-handler/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/forms-handler/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/forms-handler/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/forms-handler/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/forms-handler/**/*.ts",
          "libs/forms-handler/**/*.html",
          "libs/forms-handler/package.json"
        ]
      }
    }
  }
}
